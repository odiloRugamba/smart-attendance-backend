{
  "name": "smart-attendance-backend",
  "version": "1.0.0",
  "description": "smart-attendance-backend",
  "main": "./src/server.js",
  "scripts": {
    "dev": "node_modules/.bin/nodemon ./src/server.js --exec babel-node --",
    "start": "cross-env NODE_ENV=development npm run migrate:db && cross-env NODE_ENV=production npm run build && node dist/server.js",
    "test": "sequelize-cli --env test db:migrate:undo:all && sequelize-cli --env test db:migrate && ./node_modules/mocha/bin/mocha",
    "build": "rm -rf dist && mkdir dist && babel src -s -d dist --copy-files",
    "stop-win": "Taskkill /IM node.exe /F",
    "stop-linux": "killall node",
    "create:db": "cross-env NODE_ENV=development && node_modules/.bin/sequelize db:create",
    "create:tables": "cross-env NODE_ENV=development && npm run migrate:db && npm run seed:db",
    "migrate:db": "node_modules/.bin/sequelize db:migrate",
    "seed:db": "node_modules/.bin/sequelize db:seed:all",
    "undo:db": "node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Odilo Rugamba, Emmy Karangwa",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@hapi/joi": "^17.1.0",
    "@sendgrid/mail": "^6.5.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cloudinary": "^1.19.0",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "cryptr": "^6.0.1",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.4.2",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "path": "^0.12.7",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "pusher": "^3.0.0",
    "redis": "^3.0.2",
    "sequelize": "^5.21.4",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "sequelize-migration-pg-extras": "^1.0.1",
    "sqlite3": "^5.0.0"
  }
}
